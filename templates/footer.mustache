{{!
    Page footer.
}}
<div id="goto-top-link">
    {{! go to top is sticky to footer so needs to be sibling }}
    <a class="btn btn-lg btn-secondary" role="button" href="#">
        {{#pix}} i/up, core, {{#str}} totop, theme_boost {{/str}}{{/pix}}
    </a>
</div>
<footer id="arup-page-footer" class="py-3 bg-gray010">
    <div class="container-fluid">
        {{# output.hascohortfooters }}
        <div class="position-relative py-1">
            <hr>
            <h3 class="position-absolute bg-gray010 pr-2" style="top: 7px">{{#str}} helpsupport, theme_nova {{/str}}</h3>
        </div>
        {{> theme_nova/footer_content }}
        {{/ output.hascohortfooters }}
            <div class="main-footerlinks">
                <span class="copyright mr-1">
                     {{{output.copyright}}}
                </span>
                {{{ output.footerlinks }}}
            </div>
        <div id="course-footer">{{{ output.course_footer }}}</div>

        <div class="d-flex systemlinks align-items-center">

            {{# output.page_doc_link }}
                {{{ output.page_doc_link }}}
            {{/ output.page_doc_link }}

             <div class="tool_usertours-resettourcontainer"></div>
        </div>
        {{{ output.standard_end_of_body_html }}}
        {{{ output.standard_footer_html }}}
    </div>
</footer>